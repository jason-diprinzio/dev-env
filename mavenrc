sub="$(basename $(dirname `pwd`))"
module="$(basename `pwd`)"

if [ "${sub}" = "atom" ]
then
    version=7
else
    case $module in
        "atom") version=7;;
        "connwss") version=7;;
        "jbosslauncher") version=8;;
        *) version=6;;
    esac
fi

export JAVA_HOME=/usr/lib/jvm/java-${version}-oracle/jre

#echo $JAVA_HOME
